	<div class="row">
			<form role="form" method="post" action="/vidRead">
				<h3 class="text-center text-info">
					逾期查詢
				</h3>
				<form role="form">
				<div class="form-group">	 
					<label>
						會員編號
					</label>
					<input type="text" readonly="true" class="form-control" value="<%= items[0].cusNo %> name="cusNo"/>
				</div>
				
				<div class="form-group">		 
					<label>
						會員名稱
					</label>
					<input type="text" required class="form-control" value="<%= items[0].cusName %> name="cusName"/>
				</div>
				
				<div class="form-group">	 
					<label>
						影片編號
					</label>
					<input type="text" readonly="true" class="form-control" value="<%= items[0].vidNo %> name="vidNo"/>
				</div>
				
				<div class="form-group">		 
					<label>
						影片名稱
					</label>
					<input type="text" required class="form-control" value="<%= items[0].vidName %> name="vidName"/>
				</div>
				
				<div class="form-group">		 
					<label>
						影片類型
					</label>
					<input type="text" required class="form-control" value="<%= items[0].vidType %> name="vidType"/>
				</div>
				
				<div class="form-group">		 
					<label>
						到期時間
					</label>
					<input type="date" required class="form-control" value="<%= items[0].giveBack %> name="giveBack"/>
				</div>			
				
				<button type="submit" class="btn btn-primary">
					提交
				</button>
			</form>
		</div>
